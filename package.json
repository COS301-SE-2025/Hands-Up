{
  "name": "handsUP Application",
  "version": "1.0.0",
  "type": "module",
  "eslintConfig": {
    "env": {
      "node": true
    }
  },
  "scripts": {
    "database": "cd backend/database && docker-compose up -d --build && docker-compose run flyway migrate",
    "api": "cd backend/api && npm i && node handsUP",
    "ui": "cd frontend && npm i && npm start",
    "start": "concurrently \"npm run database\" \"npm run api\" \"npm run ui\"",
    "test-coverage": "node --no-warnings --experimental-vm-modules --experimental-require-module ./node_modules/jest/bin/jest.js --passWithNoTests --coverage",
    "lint": "eslint . --ext .js --fix"
  },
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.27.4",
    "@babel/eslint-parser": "^7.27.5",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.27.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "autoprefixer": "^10.4.21",
    "babel-eslint": "^10.1.0",
    "concurrently": "^9.1.2",
    "cypress": "^14.5.0",
    "eslint": "^9.29.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.10",
    "webpack": "^5.99.9"
  },
  "dependencies": {
    "aos": "^2.3.4",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "prop-types": "^15.8.1",
    "react-icons": "^5.5.0"
  }
}
