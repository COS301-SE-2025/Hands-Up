{
  "name": "handsUP Application",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "database": "cd backend/database && docker-compose up -d --build && docker-compose run flyway migrate",
    "api": "cd backend/api && node handsUP",
    "ui": "cd frontend && npm start",
    "start": "concurrently \"npm run database\" \"npm run api\" \"npm run ui\"",
    "test-coverage": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint . --ext .js"
  },
  "main": "index.js",
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-react": "^7.27.1",
    "@eslint/js": "^9.27.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.27.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0"
  }
}
